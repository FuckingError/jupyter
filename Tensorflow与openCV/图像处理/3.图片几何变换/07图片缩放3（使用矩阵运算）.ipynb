   "source": [
    "img = cv2.imread('../zmy.jpg',1)\n",
    "imgInfo = img.shape\n",
    "height = imgInfo[0]\n",
    "width = imgInfo[1]\n",
    "matScale = np.float32([[0.01,0,0],[0,0.01,0]])\n",
    "dst = cv2.warpAffine(img,matScale,(int(width/100),int(height/100)))\n",
    "cv2.imwrite('zmy_small.png',dst)"
